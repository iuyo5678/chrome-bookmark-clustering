 0){\u000a					 \u000a						alert('Please enter mobile number');\u000a						document.iform.phone.focus();\u000a						return false;\u000a						\u000a					}    \u000a						\u000a								mo = replaceAll( document.iform.phone.value, '-','');\u000a								mo = replaceAll( mo, ' ','');\u000a								mo = replaceAll( mo, '(','');\u000a								mo = replaceAll( mo, ')','');\u000a				\u000a				\u000a				\u000a							if ( checkMobileNumber(mo) == false ) {\u000a								alert("Your mobile number is not valid:");\u000a								return false;\u000a								\u000a							} else {\u000a								\u000a								//confirm mobile country code\u000a						\u000a								if (String(mo.slice(0,1)) != "+" ) {\u000a									if(!confirm("Please confirm these mobile number(s) are located in [China / \u4e2d\u56fd]\u005cn" + mo + "\u005cn\u005cnIf not, please prefix each number with +[country code]. \u005cnFor example, for US mobile number 9494621111 (contry code:1)\u005cnPlease enter: +19494621111.")) {\u000a				\u000a										return false;\u000a									}\u000a								} \u000a				\u000a						  }\u000a				  //\u76f4\u63a5\u7528ajax\u53d1\u624b\u673a\u000a				  //Const MAX_SMS_PER_COOKIES = 5\u000a				  //Const MAX_SMS_PER_INVITE = 200\u000a				  //Const MAX_SMS_PER_MOBILE = 1  \u000a				\u000a								createRequest();\u000a								var url = "mobile_invite_view_sms.asp?sn=" + sn + "&name=" + document.iform.name.value + "&mobile=" + getGlobalMobile(mo);\u000a				\u000a								request.open("GET", url, true);\u000a								request.onreadystatechange = function () {\u000a									if (request.readyState == 4) {\u000a										if (request.status == 200) {\u000a											var response = request.responseText; //\u83b7\u53d6\u670d\u52a1\u5668\u8fd4\u56de\u7684JSON\u5b57\u4e32\u000a											if (response == '0') {\u000a				\u000a								  document.getElementById('rtn').innerHTML = 'Send successful';\u000a				\u000a											}else if (response == '1'){\u000a												document.getElementById('rtn').innerHTML = 'Max number of SMS exceeded.(5 sms)';\u000a											}else if (response == '2'){\u000a												document.getElementById('rtn').innerHTML = 'Exceed the limit of SMS number(400 sms)';\u000a											}else if (response == '3'){\u000a								  document.getElementById('rtn').innerHTML = 'Duplicated mobile number';\u000a											}\u000a											\u000a											//document.iform.name.value = '';\u000a											document.iform.phone.value = '';\u000a											\u000a										} else {\u000a				\u000a										}\u000a									}\u000a								};\u000a						\u000a								request.send(null);\u000a				\u000a				}\u000a				\u000a				\u000a				//\u521b\u5efa\u6c42XMLHttpRequest\u5bf9\u8c61\u000a				function createRequest() {\u000a					try {\u000a						request = new XMLHttpRequest();\u000a					} catch (trymicrosoft) {\u000a						try {\u000a							request = new ActiveXObject("Msxml2.XMLHTTP");\u000a						} catch (othermicrosoft) {\u000a							try {\u000a								request = new ActiveXObject("Microsoft.XMLHTTP");\u000a							} catch (failed) {\u000a								request = false;\u000a							}\u000a						}\u000a					}\u000a					if (!request)\u000a						alert("ERROR XMLHttpRequest!");\u000a				}\u000a				\u000a				\u000a/*\u000afunction getGlobalMobile(no) {\u000a	start = String(no.slice(0,1));\u000a	if (start == "+" ) {\u000a		return String(no.slice(1,no.length))\u000a	} else {\u000a		if (start == "0") {\u000a			return  "86" + String(no.slice(1,no.length)) \u000a		} else {\u000a			return "86" + no \u000a		}\u000a	} \u000a}\u000a*/\u000a\u000afunction getGlobalMobile(no) {\u000a	var tmp_no, inter_no;\u000a	var preset_cc = '86';\u000a	start = String(no.slice(0,1));\u000a	if (start == "+" ) {\u000a		tmp_no = String(no.slice(1,no.length)); //remove leading +\u000a		if ( String(tmp_no.slice(0,preset_cc.length)) == preset_cc && String(tmp_no.slice(preset_cc.length,preset_cc.length+1)) == '0' ) {\u000a			inter_no = preset_cc + String(tmp_no.slice(preset_cc.length + 1, tmp_no.length));\u000a		} else {\u000a			inter_no = tmp_no;\u000a		}\u000a	} else {\u000a		if (start == "0") {\u000a			inter_no = preset_cc + String(no.slice(1,no.length)) \u000a		} else {\u000a			if ( String(no.slice(0,preset_cc.length)) == preset_cc ) {\u000a				if ( String(no.slice(preset_cc.length,preset_cc.length+1)) == '0'  ) {\u000a					inter_no = preset_cc + String(no.slice(preset_cc.length + 1, no.length));\u000a				} else {\u000a					inter_no =  no; \u000a				}\u000a			} else {\u000a				inter_no = preset_cc + no; \u000a			}\u000a		}\u000a	} \u000a	return inter_no;\u000a\u000a}\u000a\u000a				\u000a				function checkMobileNumber( mo ) {\u000a				\u000a				\u000a				\u000a					if (String(mo.slice(0,1)) == "+" ) {\u000a						if( isNaN(mo.slice(1,mo.length))) {\u000a							return false;\u000a						}\u000a						if( mo.length < 5) {\u000a							return false;\u000a						}\u000a					} else {\u000a						if( isNaN(mo)) {\u000a							return false;\u000a						}\u000a						if( mo.length < 6) {\u000a							return false;\u000a						}\u000a					}\u000a					return true\u000a				}\u000a				\u000a				function replaceAll(oldStr,findStr,repStr) {\u000a				\u000a				  var srchNdx = 0;\u000a								  \u000a				  var newStr = "";\u000a				  while (oldStr.indexOf(findStr,srchNdx) != -1)  \u000a				  {\u000a					newStr += oldStr.substring(srchNdx,oldStr.indexOf(findStr,srchNdx));\u000a					newStr += repStr;\u000a					srchNdx = (oldStr.indexOf(findStr,srchNdx) + findStr.length);\u000a				  }\u000a				  newStr += oldStr.substring(srchNdx,oldStr.length);\u000a				\u000a				  return newStr;\u000a				} \u000a				\u000a				\u000a            </script>\u000a            \u000a            <div align="right" style="margin-top:10px">\u000a              <div style="position:relative">\u000a                <form name="iform" method="post" >\u000a			<div id="send_sms_panel" style="display:none; position:absolute; font-size: 10pt; left: -11px; top: -1px; width: 300px; border: #F3C thin solid">                <table width="300" cellpadding="0" cellspacing="0" border="0">\u000a\u000a\u000a                    <tr>\u000a                      <td height="30" colspan="2" bgcolor="#FFFFCC" align="center">Send to mobile with FREE sms</td>\u000a                    </tr>\u000a\u000a                  <tr><td width="76%" height="30" align="right" bgcolor="#FFFFCC">Mobile NO&nbsp;&nbsp;<input type="hidden" name="name" value="YouVivid"><input type="input" name="phone" value="" size="16"></td>\u000a                    <td width="24%" align="left" bgcolor="#FFFFCC">&nbsp;&nbsp;<input type="button" name="sub" value=" Send " onclick="isIns();"></td>\u000a                  </tr>\u000a                    <tr><td height="20" colspan="2"  bgcolor="#FFFFCC" align="center"><span id="rtn" style="font-family: \u5fae\u8f6f\u6b63\u9ed1\u4f53, verdana, tahoma, arial; font-size: 10pt; color: #e10043;" class="font_01"></span></td>\u000a                      </tr>\u000a                    <tr>\u000a                      <td height="30" colspan="2" bgcolor="#FFFFCC" align="center"><img width="120" height="120" src="http://chart.apis.google.com/chart?cht=qr&chl=http%3A%2F%2Fyouvivid%2Ecom%2Fv%2F3bpgp%5Fg3yb8&chs=120x120" border="0"></td>\u000a                    </tr>\u000a                    <tr>\u000a                      <td height="30" colspan="2" bgcolor="#FFFFCC" align="center"><input type="button" onClick="javascript:toggleSendSmsPanel();" value=" Close "></td>\u000a                    </tr>\u000a                </table>\u000a            </div>\u000a            </form>\u000a          </div>            \u000a        			</div>	            \u000a                </td>\u000a              </tr>\u000a              <tr>\u000a                <td valign="middle" align="right">\u000a                </td>\u000a      